{
    "editor.fontFamily": "Noto Sans Mono CJK JP",
    "editor.fontSize": 13,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.wordWrap": "off",
    "files.autoSave": "off",
    "go.alternateTools": {
        "go-langserver": "gopls"
    },
    "go.buildFlags": [],
    "go.buildOnSave": "package",
    "go.coverOnSave": false,
    "go.formatTool": "goimports",
    "go.languageServerExperimentalFeatures": {
        "autoComplete": true,
        "format": true
    },
    "go.lintFlags": [],
    "go.lintOnSave": "file",
    "go.toolsEnvVars": {
        "GO111MODULE": "on"
    },
    "go.useCodeSnippetsOnFunctionSuggest": false,
    "go.useLanguageServer": true,
    "go.vetFlags": [],
    "go.vetOnSave": "package",
    "licenser.author": "drillbits",
    "licenser.license": "AL2",
    "python.formatting.blackPath": "~/.local/bin/black",
    "python.formatting.provider": "black",
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Path": "~/.local/bin/flake8",
    "python.linting.pylintEnabled": false,
    "python.sortImports.args": [
        "-rc"
    ],
    "python.sortImports.path": "~/.local/bin/isort",
    "terminal.integrated.fontFamily": "Noto Sans Mono CJK JP",
    "terminal.integrated.rendererType": "dom",
    "terminal.integrated.shellArgs.linux": [
        "-l"
    ],
    "terminal.integrated.shellArgs.osx": [
        "-l"
    ],
    "workbench.editor.enablePreview": false,
    "[html]": {
        "editor.formatOnSave": false
    },
    "breadcrumbs.enabled": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "window.zoomLevel": 1
}