{
	"name": "postgresql",
	"description": "postgresql VSCode part of a language server",
	"author": "JPTarquino",
	"license": "MIT",
	"version": "0.0.2",
	"publisher": "JPTarquino",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:pgsql"
	],
	"main": "./out/src/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "PostgreSQL configuration",
			"properties": {
				"postgreSql.hostName": {
					"type": "string",
					"default": "localhost",
					"description": "The PostgreSQL server to connect to when running or checking pg/sql code."
				},
				"postgreSql.username": {
					"type": "string",
					"default": "postgres",
					"description": "The PostgreSQL username to connect to when running or checking pg/sql code."
				},
				"postgreSql.dbName": {
					"type": "string",
					"default": "postgres",
					"description": "The PostgreSQL database name to connect to when running or checking pg/sql code."
				}
			}
		},
		"commands": [
			{
				"command": "postgres.executeSql",
				"title": "Execute File in Server",
				"category": "Postgres"
			}
		],
		"languages": [
			{
				"id": "pgsql",
				"extensions": [
					".pgsql"
				],
				"aliases": [
					"PGSQL"
				],
				"configuration": "./pgsql.configuration.json"
			}
		],
		"snippets": [
			{
				"language": "pgsql",
				"path": "./snippets/pgsql_snippets.json"
			}
		],
		"grammars": [
			{
				"language": "pgsql",
				"scopeName": "source.plpgsql.postgres",
				"path": "./syntaxes/pgsql.tmLanguage"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	},
	"dependencies": {
		"vscode-languageclient": "^1.1.0"
	},
	"__metadata": {
		"id": "da398f66-953d-4be6-adcd-e4344b5ccdec",
		"publisherId": "2138d502-047a-465b-8357-62122836f21c",
		"publisherDisplayName": "JPTarquino"
	}
}